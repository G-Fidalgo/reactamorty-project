{
    "presets": [
        "react-app"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "extensions": [
                    ".ios.ts",
                    ".android.ts",
                    ".ts",
                    ".ios.tsx",
                    ".android.tsx",
                    ".tsx",
                    ".jsx",
                    ".js",
                    ".json"
                ],
                "root": "./",
                "alias": {
                    "assets": "./src/assets",
                    "components": "./src/components"
                }
            }
        ]
    ]
}